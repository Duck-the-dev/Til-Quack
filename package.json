{
  "name": "vue-start",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cp env/env.dev .env && vite --mode dev",
    "clean:dist": "rimraf dist",
    "build": "vue-tsc && vite-ssg build",
    "build:dev": "pnpm clean:dist && cp env/env.dev .env && vite build --mode development",
    "build:prod": "pnpm clean:dist && cp env/env.prod .env && vite build --mode prod",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "pnpm lint:fix && cypress run --component --headless"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.26",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/head": "^1.0.26",
    "autoprefixer": "^10.4.13",
    "critters": "^0.0.16",
    "cypress": "^12.6.0",
    "daisyui": "^2.51.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-ssg": "^0.22.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.1.7"
  }
}
